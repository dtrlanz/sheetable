{
  "type": "module",
  "devDependencies": {
    "@dtrlanz/gas-bundler": "^0.3.3",
    "@types/google.script.client-side": "^0.1.5",
    "ava": "^5.3.1",
    "jsdom": "^24.0.0"
  },
  "name": "@dtrlanz/sheetable",
